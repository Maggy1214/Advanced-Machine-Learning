        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "df = pd.DataFrame(np.arange(25).reshape(5,-1), columns=list('abcde'))\n",
        "\n",
        "one_hot_df = pd.get_dummies(df['a'], prefix='a')\n",
        "\n",
        "mu = pd.concat([df, one_hot_df], axis=1)\n",
        "\n",
        "print(mu)"
